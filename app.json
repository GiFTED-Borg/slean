{
  "expo": {
    "name": "test-expo",
    "slug": "test-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/slean-logo.png",
    "scheme": "testexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/slean-logo.png",
        "backgroundColor": "#0B0C10"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/slean-logo.png",
          "imageWidth": 50,
          "resizeMode": "contain",
          "backgroundColor": "#0B0C10"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
